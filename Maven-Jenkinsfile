pipeline     

{
    agent any 
    stages
    {
        stage ('scm checkout or download the code')
        { steps { git 'https://github.com/Ayushi26Bhagat/maven-project' } }

        stage ('complie the code')
        { steps { withMaven(globalMavenSettingsConfig: '', jdk: 'LocalJDK', maven: 'LocalMaven', mavenSettingsConfig: '', traceability: true) {
           sh 'mvn complie'
}
     }}
    }
}
